{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "parcel write index.html",
    "build": "npm install --include=dev && parcel build index.html",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,html,json}\"",
    "verify": "jest --collectCoverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@parcel/transformer-react-refresh-wrap": "^2.3.2",
    "babel-jest": "^27.5.1",
    "babel-jest-glob-transform": "^1.0.2",
    "jest": "^27.5.1",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "*.{jsx,js,json}",
      "!coverag/**",
      "!dist/**",
      "!babel.config,js",
      "!index.jsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
