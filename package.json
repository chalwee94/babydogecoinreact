  "bugs": {
    "url": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-sandraasins/issues"
  },
  "homepage": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-sandraasins#readme",
  "devDependencies": {
    "babel-jest-glob-transform": "^1.0.2",
    "concurrently": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1"
  }
}
